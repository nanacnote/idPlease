{
  "name": "idplease",
  "version": "1.0.0",
  "description": "A Javascript library to make getting website visitor's information easy and simple no dependencies and can run both server side and client side.",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint src --ext ts --ext tsx --ext js --fix",
    "test": "echo \"Error: no tests installed\" && exit 1",
    "build": "rollup -c",
    "watch": "rollup -cw",
    "watchify": "watchify demo/index.js -o demo/bundle/index.js -v"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nanacnote/idPlease.git"
  },
  "keywords": [
    "user-agent",
    "visitors",
    "visitor-analytics",
    "user-information"
  ],
  "author": "Owusu K",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/nanacnote/idPlease/issues"
  },
  "homepage": "https://github.com/nanacnote/idPlease#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "eslint": "^7.7.0",
    "rollup": "^2.26.4",
    "rollup-plugin-typescript2": "^0.27.2",
    "tslib": "^2.0.1",
    "typescript": "^3.9.7",
    "watchify": "^3.11.1"
  },
  "dependencies": {}
