{
  "name": "idplease",
  "version": "1.0.0",
  "description": "A Javascript library to make getting website visitor's information easy and simple no dependencies and can run both server side and client side.",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no tests installed\" && exit 1",
    "lint": "eslint src --ext ts --ext tsx --ext js --fix",
    "build": "rollup -c",
    "watch": "rollup -cw",
    "gwatch": "grunt watch",
    "gconnect": "grunt connect",
    "watchify": "watchify docs/index.js  -p esmify -o docs/bundle/index.js -v"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nanacnote/idPlease.git"
  },
  "keywords": [
    "user-agent",
    "visitors",
    "visitor-analytics",
    "user-information"
  ],
  "author": "Owusu K",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/nanacnote/idPlease/issues"
  },
  "homepage": "https://github.com/nanacnote/idPlease#readme",
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@types/jquery": "^3.5.1",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "babelify": "^10.0.0",
    "eslint": "^7.7.0",
    "grunt": "^1.3.0",
    "grunt-babel": "^8.0.0",
    "grunt-browserify": "^5.3.0",
    "grunt-contrib-connect": "^3.0.0",
    "grunt-contrib-sass": "^2.0.0",
    "grunt-contrib-uglify": "^5.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "rollup": "^2.26.4",
    "rollup-plugin-typescript2": "^0.27.2",
    "sass": "^1.26.10",
    "tslib": "^2.0.1",
    "typescript": "^3.9.7"
  },
